{
  "name": "ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:core": "cd ../momentum-core && make run",
    "dev:backend": "cd ../momentum-backend && modd",
    "backend": "cd ../momentum-backend && ./momentum serve --publicDir=../momentum-ui/build",
    "build": "vite build",
    "build:backend": "cd ../momentum-backend && go build",
    "build:core": "cd ../momentum-core && make build",
    "generate:api-spec": "cd ../momentum-core && make generate-api-spec && cp docs/swagger.yaml ../momentum-ui/swagger.yaml",
    "generate:api-client": "openapi-generator-cli generate -i swagger.yaml -g typescript-angular -o src/lib/momentum-core-client",
    "generate:api": "npm run generate:api-spec && npm run generate:api-client",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "^0.132.5",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.30.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "pocketbase": "^0.10.2",
    "postcss": "^8.4.31",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.5.2"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.2.8",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@picocss/pico": "^1.5.10",
    "@sveltejs/adapter-node": "^1.2.1",
    "@sveltejs/adapter-static": "^2.0.2",
    "lucide-svelte": "^0.220.0",
    "pocketbase-typegen": "^1.1.9",
    "svelte-french-toast": "^1.0.4-beta.0",
    "svelte-icons-pack": "^2.1.0",
    "svelte-material-icons": "^3.0.4"
  }
}
